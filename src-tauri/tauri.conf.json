{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "extract",
  "version": "../package.json",
  "identifier": "com.tauri.extract",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "解压",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "fileAssociations": [
      {
        "ext": [
          "zip",
          "rar",
          "7z",
          "xz",
          "gz"
        ],
        "name": "压缩文件"
      }
    ]
  }
}